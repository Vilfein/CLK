<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>ClockIcon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Colored.png" />
    <None Remove="Resources\power.png" />
    <None Remove="Resources\twelve.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="ClockIcon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Colored.png" />
    <Resource Include="Resources\power.png" />
    <Resource Include="Resources\twelve.png" />
  </ItemGroup>

</Project>
